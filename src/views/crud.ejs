<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>CRUD with Trine</title>
  </head>
  <body>
    <p>SIGN UP USER</p>
    <form action="/post-crud" method="POST">
      <label for="Fullname">
        Fullname
        <input id="Fullname" type="text" name="fullNameUser" />
      </label>
      <label for="email">
        Email
        <input id="email" type="email" name="emailUser" />
      </label>
      <label for="Password">
        Password
        <input id="Password" type="password" name="passwordUser" />
      </label>
      <label for="phone">
        Phone
        <input id="phone" type="number" name="phoneUser" />
      </label>
      <label for="gender">
        Gender
        <select id="gender" name="genderUser">
          <option value="1">Male</option>
          <option value="0">Female</option>
        </select> </label
      ><label for="address">
        Address
        <input id="address" type="text" name="adressUser" />
      </label>
      <label for="role">
        Role
        <select id="role" name="idRole">
          <option value="1">Admin</option>
          <option value="2">Sale</option>
          <option value="3">Customer</option>
        </select>
      </label>
      <button type="submit">Submit</button>
    </form>
  </body>
</html>
